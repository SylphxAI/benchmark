{
  "name": "benchmark-hub",
  "version": "2.0.0",
  "description": "Unified benchmark framework for JavaScript/TypeScript libraries",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run src/index.ts",
    "benchmark": "bun run src/cli.ts",
    "register": "bun run src/cli.ts register",
    "test:single": "bun run src/cli.ts test",
    "measure-sizes": "bun run scripts/measure-bundle-sizes.ts",
    "readme:state": "bun run scripts/generate-simple-readme.ts benchmarks/state-management",
    "readme:immutability": "bun run scripts/generate-simple-readme.ts benchmarks/immutability",
    "readme:router": "bun run scripts/generate-simple-readme.ts benchmarks/router",
    "readme:all": "bun run readme:state && bun run readme:immutability && bun run readme:router",
    "update:all": "bun run measure-sizes && bun run readme:all",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "update": "bunup"
  },
  "keywords": [
    "benchmark",
    "performance",
    "framework",
    "state-management",
    "javascript",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SylphxAI/benchmark.git"
  },
  "homepage": "https://github.com/SylphxAI/benchmark#readme",
  "bugs": {
    "url": "https://github.com/SylphxAI/benchmark/issues"
  },
  "author": "Sylphx",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "^1.1.13",
    "@types/node": "^20.10.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "jotai": "^2.15.1",
    "vitest": "^2.1.5",
    "zustand": "^5.0.8"
  },
  "engines": {
    "bun": ">=1.1.0"
  }
}
